{
    "name": "maxsky/php-captcha",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MaxSky\\Captcha\\": "src/"
        },
        "files": [
            "src/Constants/API.php"
        ]
    },
    "authors": [
        {
            "name": "Max Sky",
            "email": "max940213@live.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
